<%= simple_form_for(@artwork) do |f| %>

  <%= f.input :name %>
  <%= f.input :artist %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :width %>
  <%= f.input :depth %>
  <%= f.input :height %>
  <%= f.input :weight %>
  <%= f.input :price %>
  <%= f.input :location %>
  <%= f.input :picture %>

  <%= f.submit %>

<% end %>
